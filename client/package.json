{
  "name": "frosty-react",
  "version": "1.2.1",
  "private": true,
  "dependencies": {
    "@hydrokit/button": "^0.6.0",
    "@hydrokit/checkbox": "^0.6.0",
    "@hydrokit/formfield": "^0.6.0",
    "@hydrokit/popover-menu": "^0.6.3",
    "@hydrokit/textfield": "^0.6.0",
    "@hydrokit/utils": "^0.6.3",
    "@jukben/emoji-search": "^1.1.5",
    "@types/prop-types": "^15.5.4",
    "@types/react-router-dom": "^4.3.0",
    "apollo-boost": "^0.1.12",
    "apollo-link-context": "^1.0.8",
    "formik": "^1.0.2",
    "graphql": "^0.13.2",
    "react": "^16.4.1",
    "react-apollo": "^2.1.9",
    "react-app-rewire-hot-loader": "^1.0.1",
    "react-dom": "^16.4.1",
    "react-easy-emoji": "^1.2.0",
    "react-hot-loader": "^4.3.4",
    "react-icons": "^3.0.4",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.16.0"
  },
  "scripts": {
    "start": "react-app-rewired start --scripts-version react-scripts-ts",
    "build": "react-app-rewired build --scripts-version react-scripts-ts",
    "test": "react-app-rewired test --scripts-version react-scripts-ts --env=jsdom",
    "eject": "react-scripts eject",
    "gql:download": "apollo schema:download schema.json --endpoint=http://localhost:8000/graphql",
    "gql:generate": "./node_modules/.bin/apollo codegen:generate --schema=schema.json --target=typescript --queries='**/*.tsx'",
    "gql:watch": "./node_modules/.bin/apollo codegen:generate --schema=schema.json --target=typescript --queries='**/*.tsx' --watch",
    "gql": "yarn gql:download && yarn gql:generate"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.4",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "apollo": "^1.6.0",
    "react-app-rewire-scss": "^1.0.2",
    "react-app-rewired": "^1.5.2",
    "typescript": "^2.9.2"
  },
  "proxy": "http://localhost:8000"
}
